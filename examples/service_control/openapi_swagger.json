{
  "basePath": "/",
  "consumes": [
    "application/json"
  ],
  "definitions": {
    "shelf": {
      "properties": {
        "name": {
          "type": "string"
        },
        "theme": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "theme"
      ]
    }
  },
  "host": "examples-service-control.endpoints.cloudesf-testing.cloud.goog",
  "info": {
    "description": "Configurations of gRPC Dynamic Routing using a simple Bookstore API.",
    "title": "Examples - Service Control",
    "version": "1.0.0"
  },
  "paths": {
    "/shelves": {
      "get": {
        "description": "Returns all shelves in the bookstore.",
        "operationId": "listShelves",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "List of shelves in the bookstore.",
            "schema": {
              "$ref": "#/definitions/listShelvesResponse"
            }
          }
        },
        "security": [],
        "x-google-quota": {
          "metricCosts": {
            "read-requests": 1
          }
        }
      },
      "post": {
        "description": "Creates a new shelf in the bookstore.",
        "operationId": "createShelf",
        "parameters": [
          {
            "description": "A shelf resource to create.",
            "in": "body",
            "name": "shelf",
            "required": true,
            "schema": {
              "$ref": "#/definitions/shelf"
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A newly created shelf resource.",
            "schema": {
              "$ref": "#/definitions/shelf"
            }
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    }
  },
  "produces": [
    "application/json"
  ],
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "api_key": {
      "in": "query",
      "name": "key",
      "type": "apiKey"
    }
  },
  "swagger": "2.0",
  "x-google-management": {
    "metrics": [
      {
        "displayName": "Read requests",
        "metric_kind": "DELTA",
        "name": "read-requests",
        "value_type": "INT64"
      }
    ],
    "quota": {
      "limits": [
        {
          "metric": "read-requests",
          "name": "read-limit",
          "unit": "1/min/{project}",
          "values": {
            "STANDARD": 1000
          }
        }
      ]
    }
  }
}
