{
  "swagger": "2.0",
  "info": {
    "description": "Configuration of the Path Matcher filter using a simple Bookstore API.",
    "title": "Examples - Path Matcher",
    "version": "1.0.0"
  },
  "host": "examples-path-matcher.endpoints.cloudesf-testing.cloud.goog",
  "basePath": "/",
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "https"
  ],
  "paths": {
    "/shelves/{shelf}": {
      "get": {
        "description": "Get a single shelf resource with the given name.",
        "operationId": "getShelf",
        "parameters": [
          {
            "description": "Name of the shelf to get.",
            "format": "int64",
            "in": "path",
            "name": "shelf",
            "required": true,
            "type": "integer"
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A shelf resource.",
            "schema": {
              "$ref": "#/definitions/shelf"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "shelf": {
      "properties": {
        "name": {
          "type": "string"
        },
        "theme": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "theme"
      ]
    }
  }
}