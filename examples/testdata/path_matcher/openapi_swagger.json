{
  "swagger": "2.0",
  "info": {
    "description": "Example of the Path Matcher filter using a simple Bookstore API.",
    "title": "Example - Path Matcher - Bookstore",
    "version": "1.0.0"
  },
  "host": "path-matcher.endpoints.nareddyt-cloud-esf.cloud.goog",
  "basePath": "/",
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "https"
  ],
  "paths": {
    "/libraries/{library_id}/shelves/{shelfId}": {
      "get": {
        "description": "Get a single shelf resource with the given name.",
        "operationId": "getShelf",
        "x-google-backend": {
          "address": "https://http-bookstore-edf123456-uc.a.run.app/shelves",
          "path_translation": "CONSTANT_ADDRESS"
        },
        "parameters": [
          {
            "description": "Id of the library to search.",
            "format": "int64",
            "in": "path",
            "name": "library_id",
            "required": true,
            "type": "integer"
          },
          {
            "description": "Name of the shelf to get.",
            "format": "int64",
            "in": "path",
            "name": "shelfId",
            "required": true,
            "type": "integer"
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "A shelf resource.",
            "schema": {
              "$ref": "#/definitions/shelf"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "shelf": {
      "properties": {
        "name": {
          "type": "string"
        },
        "theme": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "theme"
      ]
    }
  }
}